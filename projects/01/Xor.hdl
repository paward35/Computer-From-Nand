/**
 * Exclusive-or gate:
 * out = (not(a) and b) or (a and not(b))
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Or(a=a, b=b, out=orAB);
    Nand(a=a , b=b, out=NandAB);
    And(a=orAB, b=NandAB, out=out);
}