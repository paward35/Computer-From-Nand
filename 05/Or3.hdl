/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 */

CHIP Or3 {
    IN a, b, c;
    OUT out;

    PARTS:
    Or(a=a ,b=b ,out=aNb );
    Or(a=aNb, b=c, out=out);
}
